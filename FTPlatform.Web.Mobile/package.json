{
  "name": "h5",
  "version": "0.0.0",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "prebuild": "yarn",
    "prewatch": "yarn"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "component",
        [
          {
            "libraryName": "mint-ui",
            "style": false
          }
        ]
      ]
    ]
  },
  "dependencies": {
    "clipboard": "^1.7.1",
    "cropperjs": "^1.0.0-rc.3",
    "jquery": "^3.2.1",
    "mint-ui": "^2.2.9",
    "nativeshare": "^2.1.3",
    "vue": "^2.4.2",
    "vue-lazyload": "^1.0.6",
    "vue-router": "^2.7.0",
    "vue-scroller": "^2.2.2",
    "vuex": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^0.10.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.2",
    "css-loader": "^0.28.4",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vconsole": "^2.5.2",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.3.0",
    "webpack-merge": "^4.1.0"
  }
