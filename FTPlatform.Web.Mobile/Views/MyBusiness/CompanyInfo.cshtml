@{
    ViewBag.Title = "企业信息";
    Layout = "~/Views/Shared/_LayoutWebpack.cshtml";
}
    <link rel="stylesheet" href="~/Content/css/myBusiness/basicInformation/EnterpriseInformation.css" />
    <header class="mui-bar mui-bar-nav" style="background: #fff;">
        <a class="mui-icon-arrowleft mui-icon mui-icon-left-nav mui-pull-left" href="/mybusiness"></a>
        <h1 class="mui-title">企业信息</h1>
        <a class=" mui-icon mui-icon-right-nav mui-pull-right right_iocn" href="/mybusiness/updatecompany"><img src="~/Content/image/BasicInformation/existing.png" /></a>
    </header>
    <div class="mui-content" v-cloak>
        <div class="information_box">
            <div class="name_title">
                <span>企业名称</span><span>{{company.Name}}</span>
            </div>
            <div class="name_title">
                <span>注册资金（万元）</span><span style="width: 60%;">{{company.RegisterCapital}}</span>
            </div>

            <div class="name_title">
                <span>企业地址</span><span>{{company.Address}}</span>
            </div>

            <div class="name_title">
                <span>成立时间</span><span>{{company.RegisterTime|formatDate}}</span>
            </div>
        </div>
        <div class="information_box">
            <div class="name_title">
                <span>职位</span><span>{{user.Position}}</span>
            </div>
        </div>
    </div>
@section foot{
<script src="~/dist/member.companyinfo.bundle.js"></script>
    }