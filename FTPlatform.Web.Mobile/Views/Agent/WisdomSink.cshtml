<link rel="stylesheet" href="~/Content/css/cropper/cropper.css" />
<link rel="stylesheet" href="~/Content/css/cropper/main.css" />
<div class="writemessage">
    <header class="mui-bar mui-bar-nav">
        <a class="mui-icon-arrowleft mui-icon mui-icon-left-nav mui-pull-left" href="javascript:window.history.go(-1);"></a>
        <h1 class="mui-title">登记</h1>
    </header>
    @*<div id="sheet" class="mui-popover mui-popover-bottom mui-popover-action">
        <div class="call_box">
