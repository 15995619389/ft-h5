@{
    ViewBag.title = "评价";
    Layout = "~/Views/Shared/_LayoutWebpack.cshtml";
}

<link rel="stylesheet" href="~/Content/css/myBusiness/myNeed/MyNeeds.css" />

<header class="mui-bar mui-bar-nav" style="background: #fff;">
    <a class="mui-icon-arrowleft mui-icon mui-icon-left-nav mui-pull-left" href="javascript:window.history.go(-1)"></a>
    <h1 class="mui-title">评价</h1>
    <a href="#" class="mui-pull-right finsh-ok" id="submit">发布</a>
</header>
<input type="hidden" value="@ViewBag.id" id="did" />
<div class="mui-content" style="background: #fff;" v-cloak>
    <input type="hidden" id="TargerUserId" v-bind:value="demand.UserId" />
    <section class="evalut-wrap">
        <div class="eva-row" style="border-top:1px solid #ddd ;">
            <div class="left-pc">
                <img src="/Content/image/site.png" />
            </div>
            <div class="right-text">{{demand.Title}}</div>
        </div>
        <div class="entereva">
            <textarea id="Appraise" placeholder="请输入评语"></textarea>
        </div>
    </section>
    <div style="background: #f5f5f5; width: 100%; height: 10px;"></div>
    <section class="satisfaction">
        <h4>请对"{{demandname}}"进行评价</h4>
        <div class="stcti-box">
            <ul>
                <li>
                    <div class="stc-label">服务态度</div>
                    <div class="stc-star">
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                    </div>
                </li>
                <li>
                    <div class="stc-label">沟通顺畅度</div>
                    <div class="stc-star">
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                    </div>
                </li>
                <li>
                    <div class="stc-label">反馈速度</div>
                    <div class="stc-star">
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                        <i class="grey-star"></i>
                    </div>
                </li>
            </ul>
        </div>

    </section>
</div>
@section foot{
<script src="~/dist/expert.critiquedemanduser.bundle.js"> </script>
    }

